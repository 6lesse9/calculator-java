pipeline {
    agent any

    stages {
        stage ('docker_build') {
            steps {
                dir ('dockerfiles') {
                    sh 'docker build -t calculator1.0 .'
                }
            }
         stage ('docker_run') {
            steps {
                    sh 'docker run -name calculator1.0 -t -i calculator1.0'
                }
            }
        }
    }
}
